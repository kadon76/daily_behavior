<%= render 'shared/error_messages', object: f.object %>

<%= f.label :pos1, "Positive" %>
<%= f.hidden_field :pos1_name, value: "Positive" %>
<%= f.select(:pos1, options_for_select([['0', 0], ['1', 1]], @behavior.pos1))  %>

<%= f.label :neg1, "Negative1" %>
<%= f.hidden_field :neg1_name, value: "Negative1" %>
<%= f.select(:neg1, options_for_select([['0', 0], ['1', 1]], @behavior.neg1))  %>

<%= f.label :neg2, "Negative2" %>
<%= f.hidden_field :neg2_name, value: "Negative2" %>
<%= f.select(:neg2, options_for_select([['0', 0], ['1', 1]], @behavior.neg2))  %>

<%= f.label :cor1, "Correction" %>
<%= f.hidden_field :cor1_name, value: "Correction" %>
<%= f.select(:cor1, options_for_select([['0', 0], ['1', 1]], @behavior.cor1))  %>

<%= f.label :record_time, "Behavior Record Time" %>
<%= f.datetime_select :record_time, { ampm: true, default: @behavior.record_time }%>

